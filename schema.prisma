datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AnotherPost {
  authorId    Int?
  id          Int          @id @default(autoincrement())
  title       String?
  AnotherUser AnotherUser? @relation(fields: [authorId], references: [id])
}

model AnotherUser {
  id          Int           @id @default(autoincrement())
  name        String?
  AnotherPost AnotherPost[]
}
